{
	"name": "flexsyscz/localization",
	"description": "Localization support for PHP (Nette) web applications.",
	"keywords": ["flexsyscz", "date", "time", "date-time"],
	"homepage": "https://www.flexsys.cz",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "Václav Lukeš",
			"homepage": "https://www.vaclavlukes.cz"
		}
	],
	"require": {
		"php": ">=8.1",
		"tracy/tracy": "^2.10",
		"nette/utils": "^4.0",
		"nette/neon": "^3.4",
		"latte/latte": "^3.0",
		"flexsyscz/file-system": "^2.0",
		"ext-iconv": "*"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^2.0",
		"nette/coding-standard": "^3.3"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
